{
  "name": "digibytejs-lib",
  "version": "1.4.4",
  "description": "Client-side Digibyte JavaScript library",
  "main": "./src/index.js",
  "keywords": [
    "digibyte",
    "browser",
    "client",
    "library"
  ],
  "contributors": [
    {
      "name": "Esotericizm",
      "email": "esotericizm@cryptoservices.net",
      "url": "http://cryptoservices.net"
    }
  ],
  "scripts": {
    "compile": "browserify ./src/index.js -s digibyte | uglifyjs > digibytejs-min.js",
    "coverage": "istanbul cover _mocha -- test/*.js",
    "coveralls": "npm run-script coverage && coveralls < coverage/lcov.info",
    "integration": "mocha --reporter list test/integration/*.js",
    "jshint": "jshint --config jshint.json src/*.js ; true",
    "test": "npm run-script unit",
    "unit": "istanbul test mocha -- --reporter list test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DigiByte-Team/digibytejs-lib"
  },
  "dependencies": {
    "bigi": "^1.4.0",
    "bs58check": "^1.0.4",
    "ecurve": "^1.0.0",
    "typeforce": "^0.1.0"
  },
  "devDependencies": {
    "async": "^0.9.0",
    "browserify": "^8.1.0",
    "bs58": "^2.0.1",
    "cb-helloblock": "^0.4.10",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.11",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": "0.0.1",
    "sinon": "^1.12.2",
    "uglify-js": "^2.4.16"
  }
}
